{
  "expo": {
    "name": "aitravel-planner",
    "slug": "aitravel-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "travelo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Loadergif.gif",
      "resizeMode": "contain",
      "backgroundColor": "#151718",
      "width": 50,
      "height": 50
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#151718"
      },
      "package": "com.flowentech.aitravelplanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "811d02d1-33a4-4888-96f8-f8b808f2d6f3"
      }
    }
  }
}
